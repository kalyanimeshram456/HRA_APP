<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_main</item>
        <item name="colorPrimaryDark">@color/color_main</item>
        <item name="colorAccent">@color/color_main</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>//TODO - added for blank screen
    </style>
    <!-- Base application theme. -->
    <style name="AppThemeSlider" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_main</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/color_main</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="Editext.Theme" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/_14sp</item>
        <item name="colorAccent">@color/back_text_colour</item>
        <item name="android:textColor">@color/text_disabled</item>
        <item name="android:textColorHint">@color/back_text_colour</item>
        <!--<item name="android:fontFamily">@font/poppins_bold</item>-->
        <item name="colorPrimary">@color/back_text_colour</item>
        <item name="colorPrimaryDark">@color/back_text_colour</item>
    </style>
    <!-- RatingBar -->
    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_full</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_full</item>
        <item name="android:minHeight">@dimen/_50dp</item>
        <item name="android:maxHeight">@dimen/_50dp</item>
    </style>

    <style name="AppSearchView" parent="Widget.AppCompat.SearchView" >
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">@dimen/_14sp</item>
    </style>

    <style name="CustomTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/_16sp</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppThemeBackground" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_main</item>
        <item name="colorAccent">@color/color_main</item>
        <item name="android:windowBackground">@drawable/bg_login_</item>
    </style>

    <!-- Style For Custom Dialog -->
    <style name="ThemeDialogCustom">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>

    <style name="AppTheme.White" parent="AppTheme">
        <!--Required Api 21 above-->
        <item name="android:windowLightStatusBar">true</item>
        <item name="colorPrimary">@color/color_main</item>
        <item name="colorPrimaryDark">@color/color_main</item>
        <item name="colorAccent">@color/color_main</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <!--  <item name="android:windowBackground">@drawable/get_started_theme</item>-->
    </style>

