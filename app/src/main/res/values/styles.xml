    <style name="AppTheme.FullScreen" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="colorPrimary">@color/color_main</item>
        <item name="colorPrimaryDark">@color/color_main</item>
        <item name="colorAccent">@color/color_main</item>
       <!-- <item name="android:statusBarColor">@color/color_main</item>--><!--#E0E7FB-->
        <item name="statusBarBackground">@drawable/layout_round_shape_status_bar</item>
        <item name="android:windowLightStatusBar">true</item>
        <!--
                <item name="android:statusBarColor">@android:color/transparent</item>
        -->

    </style>

    <style name="NavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemTextAppearanceActive">@style/ActiveText</item>
        <item name="itemTextAppearanceInactive">@style/InactiveText</item>
        <item name="android:background">@color/deep_red</item>
        <item name="itemBackground">@drawable/draw_nav_selectore</item>
        <item name="itemIconTint">@color/deep_red</item>
        <item name="itemTextColor">@color/deep_red</item>
        <item name="paddingStart">@dimen/_2dp</item>
        <item name="paddingEnd">@dimen/_2dp</item>
    </style>
    <style name="ActiveText">
        <item name="android:textColor">@color/deep_red</item>
    </style>
    <style name="InactiveText">
        <item name="android:textColor">@color/deep_red</item>
    </style>
    <style name="AppTheme.BottomBar">
        <item name="colorPrimary">@color/Light_Yellow</item>
    </style>

