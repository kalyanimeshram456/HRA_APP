    <attr format="color" name="ss_circleColor"/>
    <attr format="color" name="ss_countTextColor"/>
    <attr format="color" name="ss_countBackgroundColor"/>
    <attr format="string" name="ss_countTypeface"/>
    <attr format="color" name="ss_rippleColor"/>
    <attr format="color" name="ss_shadowColor"/>
</declare-styleable>
</resources>