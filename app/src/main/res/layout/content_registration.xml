                            android:textColor="@color/black_heading"
                            android:textColorHint="@color/app_gray"
                            android:textSize="@dimen/_14sp" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="2">

                            <androidx.appcompat.widget.AppCompatRadioButton
                                android:id="@+id/radioYear"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/_20dp"
                                android:layout_centerVertical="true"
                                android:backgroundTint="@color/color_main"
                                android:shadowColor="@color/color_main" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tvYear"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/_0dp"
                                android:layout_toRightOf="@+id/radioYear"
                                android:fontFamily="@font/gilroy_semibold"
                                android:inputType="textMultiLine"
                                android:text="@string/scr_lbl_yearly_plan"
                                android:textColor="@color/black_heading"
                                android:textSize="@dimen/_14sp" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
